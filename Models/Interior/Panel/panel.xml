<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    <object-name>leather</object-name>
    <object-name>visses</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- Interior lighting -->
  <animation>
    <type>material</type>
    <object-name>panel</object-name>
    <object-name>leather</object-name>
    <object-name>visses</object-name>
      <condition>
        <equals>
          <property>controls/lighting/panel</property>
          <value>1</value>
        </equals>
      </condition>
    <emission>
      <red>   0.27 </red>
      <green> 0.17 </green>
      <blue>  0.12 </blue>
      <factor-prop>/controls/lighting/brightness</factor-prop>
    </emission>
  </animation>

