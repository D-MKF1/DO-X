<!-- COMMON SOUNDS -->

    <propeller>
      <name>propeller1</name>
      <mode>looped</mode>
      <path>Sounds/turboprop1.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.2</min>
        <max>0.8</max>
      </volume>
      <volume>
        <property>/sim/current-view/internal</property>
        <factor>0.5</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/mp-osi</property>
        <factor>0.0036</factor>
        <min>0.5</min>
        <max>2.0</max>
        <offset>0.9</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </propeller>
    <propeller>
      <name>propeller2</name>
      <mode>looped</mode>
      <path>Sounds/turboprop1.wav</path>
      <property>/engines/engine[1]/running</property>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.2</min>
        <max>0.8</max>
      </volume>
      <volume>
        <property>/sim/current-view/internal</property>
        <factor>0.5</factor>
      </volume>
      <pitch>
        <property>/engines/engine[1]/mp-osi</property>
        <factor>0.0036</factor>
        <min>0.5</min>
        <max>2.0</max>
        <offset>0.9</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </propeller>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>/Sounds/wave.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>/gear/gear[3]/wow</property>
          <property>/gear/gear[4]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.25</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <squeal>
     <name>water</name>
     <path>Sounds/wave.wav</path>
     <condition>
       <or>
         <property>/gear/gear[0]/wow</property>
         <property>/gear/gear[1]/wow</property>
         <property>/gear/gear[2]/wow</property>
         <property>/gear/gear[3]/wow</property>
         <property>/gear/gear[4]/wow</property>
       </or>
     </condition>
     <volume>
       <property>/velocities/speed-down-fps</property>
       <factor>0.5</factor>
       <max>1.0</max>
     </volume>
     <volume>
       <property>/velocities/speed-down-fps</property>
       <factor>0.5</factor>
       <max>1.0</max>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
     </volume>
     <volume>
       <internal>dt_stop</internal>
       <factor>0.05</factor>
       <max>1.0</max>
     </volume>
     <pitch>
       <property>/velocities/airspeed-kt</property>
       <factor>0.0025</factor>
       <offset>0.5</offset>
       <max>1.0</max>
     </pitch>
