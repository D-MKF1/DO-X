<?xml version="1.0"?>
 
<PropertyList>

 <fx>
 
<!-- ####################################  Left site engines / larboard ######################################## -->

  <crank>
   <name>engstart0</name>
   <path>Sounds/engstart.wav</path>
   <condition>
     <property>/engines/engine[0]/cranking</property>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <offset>0.4</offset>
   </volume>
  </crank>

  <crank>
   <name>crank0</name>
   <mode>looped</mode>
   <path>Sounds/cranking.wav</path>
   <condition>
    <or>
     <and>
      <property>/engines/engine[0]/cranking</property>
      <not>
       <property>/engines/engine[0]/running</property>
      </not>
     </and>
    </or>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <offset>0.5</offset>
   </volume>
  </crank>

  <crank>
   <name>cough0</name>
   <path>Sounds/coughing.wav</path>
   <condition>
     <property>/engines/engine[0]/running</property>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <offset>0.3</offset>
   </volume>
  </crank> 

<!-- engine stop only ask over magnetos setting for the moment :-( -->
  <crank>
   <name>stop0</name>
   <path>Aircraft/DO-X/Sounds/engine_down.wav</path>
   <condition>
      <equals>
     <property>controls/engines/engine[0]/magnetos</property>
      <value>0</value>
      </equals>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <offset>0.007</offset>
   </volume>
  </crank>

<!-- internal -->

  <engine>
   <name>engine0</name>
   <mode>looped</mode>
   <path>Sounds/wasp.wav</path>
   <condition>
    <and>
     <or>
      <and>
       <property>/engines/engine[0]/running</property>
       <greater-than-equals>
        <property>/engines/engine[0]/mp-osi</property>
        <value>1.75</value>
       </greater-than-equals>
      </and>
     </or>
     <property>/sim/current-view/internal</property>
    </and>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/engines/engine[0]/mp-osi</property>
    <factor>0.009</factor>
    <min>0.01</min>
    <max>0.15</max>
    <offset>0.07</offset>
   </volume>
   <pitch>
    <property>/controls/engines/average-rpm-left</property>
    <factor>0.0003</factor>
    <min>0.1</min>
    <max>2.0</max>
    <offset>0.3</offset>
   </pitch>
  </engine>


  <engine>
   <name>idle0</name>
   <mode>looped</mode>
   <path>Sounds/wasp.wav</path>
   <condition>
    <and>
     <or>
      <and>
       <property>/engines/engine[0]/running</property>
       <less-than>
        <property>/engines/engine[0]/mp-osi</property>
        <value>13.5</value>
       </less-than>
      </and>
     </or>
     <property>/sim/current-view/internal</property>
    </and>
   </condition>
   <position>
    <x>15.0</x>
    <y>-5.0</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/engines/engine[0]/mp-osi</property>
    <factor>0.009</factor>
    <min>0.11</min>
    <max>0.21</max>
    <offset>0.065</offset>
   </volume>
   <pitch>
    <property>/controls/engines/average-rpm-left</property>
    <factor>0.0009</factor>
    <min>0.55</min>
    <max>4.5</max>
    <offset>0.3</offset>
   </pitch>
  </engine>


<!-- external -->

  <engine>
   <name>engine0 external</name>
   <mode>looped</mode>
   <path>Sounds/wasp.wav</path>
   <condition>
    <and>
     <or>
      <and>
