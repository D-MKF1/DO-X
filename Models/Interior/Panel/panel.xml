    <font-resolution>
      <width type="int">32</width>
      <height type="int">32</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m> 0.117 </x-m> <!-- near to panel -->
      <y-m> -0.635 </y-m> <!-- right -->
      <z-m>  0.450 </z-m> <!-- up -->
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </text>

  <!-- Labels activated by hotspots -->
  <animation>
    <type>material</type>
    <object-name>LabelRDF</object-name>
    <object-name>LabelAI</object-name>
    <object-name>LabelMiddle</object-name>
    <object-name>LabelThrottle</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.8 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LabelRDF</object-name>
    <object-name>LabelAI</object-name>
    <object-name>LabelMiddle</object-name>
    <object-name>LabelThrottle</object-name>
    <condition>
      <property>/sim/panel-hotspots</property>
    </condition>
  </animation>

