<!-- cannot disable click in external views, otherwise heard when ctrl-V -->

  <click>
   <name>click on</name>
   <path>Aircraft/DO-X/Sounds/cptonboard.wav</path>
   <condition>
    <property>/instrumentation/generic/click</property>
   </condition>
   <volume>
    <max>1.0</max>
   </volume>
  </click>

  <click>
   <name>click off</name>
   <path>Aircraft/DO-X/Sounds/cptonboard.wav</path>
   <condition>
    <not>
     <property>/instrumentation/generic/click</property>
    </not>
   </condition>
   <volume>
    <max>1.0</max>
   </volume>
  </click>

   <!-- for radio direction finder -->
    <rdfN>
     <name>NAV-sound</name>
     <mode>looped</mode>
     <path>Aircraft/DO-J-II/Sounds/rdf.wav</path>
     <condition>
      <and>
        <property>/instrumentation/rdf/power-on</property>
        <property>/instrumentation/nav/power-btn</property>
        <property>/instrumentation/rdf/headset/position</property>        
        <equals>
           <property>/instrumentation/rdf/frequency-select-knob</property>
           <value>1</value>
        </equals>
        <or>
         <equals>
           <property>/instrumentation/nav/in-range</property>
           <value>1</value>
         </equals>
         <equals>
           <property>/instrumentation/rdf/cat-ship-in-range</property>
           <value>1</value>
         </equals>
        </or>
       </and>
     </condition>
      <pitch>
          <property>/instrumentation/nav/volume</property>
          <factor>1.4</factor>
      </pitch>
      <volume>
        <property>/instrumentation/nav/volume</property>
        <factor>1</factor>
      </volume>
    </rdfN>

    <rdfA>
     <name>ADF-sound</name>
     <mode>looped</mode>
     <path>Aircraft/DO-J-II/Sounds/rdf.wav</path>
     <condition>
      <and>
